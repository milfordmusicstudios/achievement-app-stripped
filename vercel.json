{
  "redirects": [
    {
      "source": "/Summer",
      "has": [
        { "type": "host", "value": "milfordmusic.com" }
      ],
      "destination": "https://awards.milfordmusic.com/summer.html",
      "permanent": true
    },
    {
      "source": "/Summer",
      "destination": "/summer.html",
      "permanent": false
    },
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "achievement-app-stripped.vercel.app" }
      ],
      "destination": "https://awards.milfordmusic.com/$1",
      "permanent": true
    }
  ]
}
